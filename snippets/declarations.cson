".source.js":
  "var":
    prefix: "v"
    body: "var ${1:name}"
  "var assignment":
    prefix: "v="
    body: "var ${1:name} = ${2:value}"
  "let":
    prefix: "l"
    body: "let ${1:name}"
  "let assignment":
    prefix: "l="
    body: "let ${1:name} = ${2:value}"
  "const":
    prefix: "co"
    body: "const ${1:name}"
  "const":
    prefix: "co="
    body: "const ${1:name} = ${2:value}"
